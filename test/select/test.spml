<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>

<sp:select name="sp_choice">
	<sp:option value="sitepark">sitepark</sp:option>
	<sp:option value="develop">develop</sp:option>
	<sp:loop collection="_collection" item="_i">
		<sp:option value="${_i.name.toLowerCase()}"><sp:print name="_i.name"/></sp:option>
	</sp:loop>
</sp:select>

