<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>
<sp:diff name="result" from="abc" to="abc cde" revised="_revised" original="_original" lookup="false"/>
<sp:loop collection="result" item="it">
	<sp:condition>
		<sp:if name="it.isNew">
			got changed: <sp:print name="it.revised"/>,
		</sp:if>
		<sp:elseif name="it.isChanged">
			has changed from <sp:print name="it.original"/> to <sp:print name="it.revised"/>,
		</sp:elseif>
		<sp:elseif name="is.isDeleted">
			got deleted: <sp:print name="it.original"/>,
		</sp:elseif>
		<sp:else>
			is unchanged: <sp:print name="it.original"/>,
		</sp:else>
	</sp:condition>
</sp:loop>

