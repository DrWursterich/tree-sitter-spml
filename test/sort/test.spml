<%-- sort ascending: --%>
<sp:sort name="sortedList" collection="names" sequences="desc"/>
<sp:loop collection="sortedList" item="it">
	<sp:print name="it"/>
</sp:loop>

<%-- sort descending: --%>
<sp:sort name="sortedList" collection="names"/>
<sp:iterator collection="sortedList" item="it">
	<sp:print name="it"/>
</sp:iterator>

