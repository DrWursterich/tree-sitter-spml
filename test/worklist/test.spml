<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>

<sp:worklist name="_worklist" type="content" role="_role" user="_user" element="_element"/>

<sp:loop collection="_worklist" item="_it">
	<sp:print name="it.timestamp" dateformat="dd.MM.yyyy HH.mm"/>
		sender: <sp:print name="_it.senderID.name"/>
	<sp:print name="_it.message"/>
</sp:loop>

