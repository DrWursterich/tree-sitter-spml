<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>

<sp:subinformation name="sp_description">
	<sp:select name="sp_type">
		<sp:loop collection="_types" item="_type">
			<sp:option value="_type.value"><sp:print name="_type.label"/></sp:option>
		</sp:loop>
	</sp:select>
	<sp:subinformation name="sp_teaserIcon">
		<sp:checkbox name="sp_hide" checked="true"/>
	</sp:subinformation>
</sp:subinformation>

