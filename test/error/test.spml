<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>
<sp:set name="_err" condition="false"/>
<sp:error code="LGIN-E011">
	<sp:set name="_err" condition="true"/>
</sp:error>

<%-- check for errors --%>
<sp:condition>
	<sp:if name="_err">
		An error has occured.
	</sp:if>
	<sp:else>
		<sp:include uri="/templates/sectionTypes/something.spml"/>
	</sp:else>
</sp:condition>
