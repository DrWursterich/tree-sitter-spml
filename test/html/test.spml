<%@ page language="java" pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"
%><%@ taglib uri="http://www.sitepark.com/taglibs/core" prefix="sp"
%>

<!DOCTYPE html "about:legacy-compat">

<!--
	this is a html comment.
	it can in fact contain spml tag that will be evaluated!
	author: <sp:print name="_author"/>
	<%-- also spml comments work here --%>
-->

<script type="application/javascript">
	// some sample js
	console.log('test!');
</script>

<style type="application/css">
	/* some sample css */
	div {
		background-color: #AACC88;
	}
</style>

<sp:linkedInformation name="adsf"/>

<div asdf="asdf">
	asddf
	<sp:for index="_i" from="1" to="3">
		<span>index: &lt;<sp:print name="_i"/>&gt;</span><br>
	</sp:for>
</div>

<sp:set name="_test" object="${_some.${!{anchor}.toString()}}"/>
<sp:set name="start" value="10"/>
<sp:set name="end" value="4"/>
<sp:set name="step" value="-1"/>
<ul attribute_without_value>
	<sp:for index="_i" from="${start}" condition="${_i} >= (${end} + 1)" step="${step}">
		<%-- as li is an optional tag its end tag can be omitted here --%>
		<li><span>index: <sp:print name="_i"/></span>
		<sp:break/>
	</sp:for>
</ul>

