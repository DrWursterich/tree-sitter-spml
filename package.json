{
  "name": "tree-sitter-spml",
  "version": "0.0.1",
  "description": "SPML grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "jsp"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "homepage": {
    "url": "https://github.com/DrWursterich/tree-sitter-spml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DrWursterich/tree-sitter-spml.git"
  },
  "bugs": {
    "url": "https://github.com/DrWursterich/tree-sitter-spml/issues"
  },
  "author": "Mario Sch√§per <schaeper@sitepark.com>",
  "license": "ISC",
  "dependencies": {
    "node-addon-api": "^8.2.2",
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.4",
    "prebuildify": "^6.0.1"
  },
  "scripts": {
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "text.spml.basic",
      "file-types": [
        "spml"
      ],
      "injection-regex": "spml",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections"
      ]
    }
  ]
}
